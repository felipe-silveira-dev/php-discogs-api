{
    "name": "felipe-silveira-dev/php-discogs-api",
    "description": "The Discogs API makes it easy for developers to communicate with the Discogs platform",
    "type": "library",
    "keywords": ["discogs", "api"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.0 || ^8.0",
        "guzzlehttp/guzzle-services": "^1.2",
        "guzzlehttp/oauth-subscriber": "^0.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5.19 || ^9.5.8"
    },
    "authors": [
        {
            "name": "Richard van den Brand (original author) / Felipe Silveira (forked)",
            "email": "richard@vandenbrand.org"
        }
    ],
    "autoload": {
        "psr-0": { "Discogs": "lib/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
